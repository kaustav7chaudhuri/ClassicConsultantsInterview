@model InterviewTestRunMVC.Models.Student

<html>
<head>
    <h1>Update Student</h1>
</head>
<body>
    <form asp-action="DeleteStudent" asp-controller="Interview" method="post">
  
        <input asp-for="StudentID" class="form-control"  hidden/>
        <br />
        <label asp-for="StudentName"></label>
        <input asp-for="StudentName" class="form-control" disabled />
        <br />
        <label asp-for="Department"></label>
        <input asp-for="Department" class="form-control" disabled />
        <br />
        <label asp-for="CollegeName"></label>
        <input asp-for="CollegeName" class="form-control"  disabled/>
        <br />
        <label asp-for="RollNumber"></label>
        <input asp-for="RollNumber" class="form-control" disabled />
        <br />
        <label asp-for="StudentType"></label>
        <input asp-for="StudentType" class="form-control" disabled />
        <input type="submit" class="btn btn-danger" value="Delete Student" />
    </form>
</body>
</html>